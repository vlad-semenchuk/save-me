{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/save-me-bot/src",
  "monorepo": true,
  "root": "apps/save-me-bot",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/save-me-bot/tsconfig.app.json"
  },
  "projects": {
    "save-me-bot": {
      "type": "application",
      "root": "apps/save-me-bot",
      "entryFile": "main",
      "sourceRoot": "apps/save-me-bot/src",
      "compilerOptions": {
        "tsConfigPath": "apps/save-me-bot/tsconfig.app.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/shared/config",
      "entryFile": "index",
      "sourceRoot": "libs/shared/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/config/tsconfig.lib.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/shared/core",
      "entryFile": "index",
      "sourceRoot": "libs/shared/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/core/tsconfig.lib.json"
      }
    },
    "save-me-bot-lib": {
      "type": "library",
      "root": "libs/save-me-bot",
      "entryFile": "index",
      "sourceRoot": "libs/save-me-bot/src",
      "compilerOptions": {
        "tsConfigPath": "libs/save-me-bot/tsconfig.lib.json"
      }
    }
  }
}